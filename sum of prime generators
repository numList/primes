import math as m
def sum_gen(k):
    numList = []
    for i in range(2,m.ceil(k/2)+1):
        x = k - i
        if is_prime(x) and is_prime(i):
            numList.append((i,x))
    return numList

def is_prime(k):
    for i in range(2,m.ceil(m.sqrt(k))+1):
        if k%i==0:
            return False
    return True

